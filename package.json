{
  "name": "polymarket-quant",
  "version": "2.0.0",
  "description": "Quantitative trading platform for Polymarket 15-minute crypto markets (Modular System)",
  "type": "module",
  "scripts": {
    "live": "node scripts/run_live_trading.mjs",
    "test": "vitest",
    "test:run": "vitest run",
    "test:integration": "vitest run __tests__/integration",
    "exec:simple": "node scripts/test_order_simple.mjs",
    "exec:check": "node scripts/test_order_simple.mjs check",
    "exec:book": "node scripts/test_order_simple.mjs book",
    "exec:roundtrip": "node scripts/test_roundtrip.mjs",
    "exec:roundtrip:dry": "node scripts/test_roundtrip.mjs --dry-run",
    "launch:config": "node scripts/launch-config.mjs",
    "preflight": "node scripts/preflight.mjs",
    "backtest": "node scripts/backtest.mjs",
    "verify": "node scripts/verify.mjs",
    "verify:edge": "node scripts/verify-edge.mjs",
    "deploy": "node scripts/deploy.mjs"
  },
  "dependencies": {
    "@polymarket/clob-client": "^5.2.1",
    "axios": "^1.6.8",
    "better-sqlite3": "^9.6.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.16.0",
    "simple-statistics": "^7.8.0",
    "uuid": "^9.0.0",
    "ws": "^8.16.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "vitest": "^4.0.18"
  }
