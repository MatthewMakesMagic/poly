{
  "name": "polymarket-quant",
  "version": "1.0.0",
  "description": "Quantitative trading platform for Polymarket 15-minute crypto markets",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "collect": "node scripts/start_collector.js",
    "analyze": "node scripts/run_analysis.js",
    "backtest": "node scripts/backtest.js",
    "paper": "node scripts/paper_trade.js",
    "dashboard": "node src/dashboard/server.js",
    "15m": "node scripts/crypto-15min-tracker.js status",
    "15m:stream": "node scripts/crypto-15min-tracker.js stream btc",
    "15m:watch": "node scripts/crypto-15min-tracker.js watch",
    "exec:test": "node scripts/test_live_order.mjs",
    "exec:simple": "node scripts/test_order_simple.mjs",
    "exec:check": "node scripts/test_order_simple.mjs check",
    "exec:book": "node scripts/test_order_simple.mjs book",
    "exec:buy": "node scripts/test_order_simple.mjs buy",
    "exec:sell": "node scripts/test_order_simple.mjs sell",
    "exec:roundtrip": "node scripts/test_roundtrip.mjs",
    "exec:roundtrip:dry": "node scripts/test_roundtrip.mjs --dry-run",
    "exec:v6": "node scripts/test_order_v6.mjs",
    "exec:test:connection": "node scripts/test_live_order.mjs connection",
    "exec:test:limit": "node scripts/test_live_order.mjs limit",
    "exec:test:market": "node scripts/test_live_order.mjs market",
    "exec:dry-run": "node scripts/run_execution_engine.mjs --dry-run",
    "exec:paper": "node scripts/run_execution_engine.mjs --paper",
    "exec:live": "node scripts/run_execution_engine.mjs --live",
    "live": "node scripts/run_live_trading.mjs",
    "live:paper": "node scripts/run_live_trading.mjs --paper",
    "live:aggressive": "LIVE_STRATEGY=SpotLag_Aggressive node scripts/run_live_trading.mjs",
    "docker:build": "docker build -t poly-trading .",
    "docker:run": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:stop": "docker-compose down",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@polymarket/clob-client": "^5.2.1",
    "@supabase/supabase-js": "^2.91.1",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "ethers": "^6.16.0",
    "express": "^4.18.0",
    "global-agent": "^3.0.0",
    "https-proxy-agent": "^7.0.6",
    "pg": "^8.17.2",
    "simple-statistics": "^7.8.0",
    "uuid": "^9.0.0",
    "ws": "^8.16.0",
    "better-sqlite3": "^9.6.0"
  },
    "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "vitest": "^4.0.18"
  }
}
